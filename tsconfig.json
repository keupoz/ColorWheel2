{
    "compilerOptions": {
        "esModuleInterop": true,
        "lib": [
            "es2017",
            "dom"
        ],
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "resolveJsonModule": true,
        "strict": true,
        "target": "es5"
    }
}
